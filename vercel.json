{
  "rewrites": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "^(www\\.)?metraap\\.com$"
        }
      ],
      "destination": "/frontend/new-landing-v2.html"
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": ".*\\.metraap\\.com"
        }
      ],
      "missing": [
        {
          "type": "host",
          "value": "www.metraap.com"
        },
        {
          "type": "host",
          "value": "metraap.com"
        }
      ],
      "destination": "/frontend/index.html"
    }
  ]
}