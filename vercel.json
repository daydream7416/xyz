{
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "rewrites": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": ".*\\.metraap\\.com"
        }
      ],
      "missing": [
        {
          "type": "host",
          "value": "www.metraap.com"
        },
        {
          "type": "host",
          "value": "metraap.com"
        }
      ],
      "destination": "/new-landing-v2.html"
    }
  ]
}