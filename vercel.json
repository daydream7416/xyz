{
  "rewrites": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "(?!www\\.)(?<subdomain>.+)\\.metraap\\.com"
        }
      ],
      "destination": "/landing/main.html"
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "(www\\.)?metraap\\.com"
        }
      ],
      "destination": "/kayit.html"
    }
  ]
}