{
  "rewrites": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "metraap.com"
        }
      ],
      "destination": "/kayit.html"
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.metraap.com"
        }
      ],
      "destination": "/kayit.html"
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "*.metraap.com"
        }
      ],
      "destination": "/landing/main.html"
    }
  ]
}